	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "go!!",
			"runtimeExecutable": "nodemon",
			"program": "${file}",
			"restart": true,
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		},
		{
			"command": "npm run prune && npm run func",
			"name": "func",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"command": "npm run deploy",
			"name": "deploy",
			"request": "launch",
			"type": "node-terminal"
		}
	]
}
